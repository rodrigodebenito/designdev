package nyt.util {	/*	 * A collection of static methods for common string operations.	*/		public class String2 {		public static function count(haystack:String, needle:String, offset:Number=0):Number {			var len:Number = haystack.length			var res:Number = 0;			haystack = haystack.slice(offset, len);            while (haystack.length > 0 && haystack.indexOf(needle) != -1) {				haystack = haystack.slice((haystack.indexOf(needle) + needle.length));                res++;            }	            return res;        }		public static function generateRandomString(newLength:uint = 1, userAlphabet:String = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"):String{			var alphabet:Array = userAlphabet.split("");			var alphabetLength:int = alphabet.length;			var randomLetters:String = "";			for (var i:uint = 0; i < newLength; i++){				randomLetters += alphabet[int(Math.floor(Math.random() * alphabetLength))];			}			return randomLetters;		}				public static function trim(str:String):String {						while(str.length > 0 && (str.charAt(0) == " " || str.charAt(0) == "\t")) str = str.substring(1);			while(str.length > 0 && (str.charAt(str.length-1) == " " || str.charAt(str.length-1) == "\t")) str = str.substring(0,str.length-1);						return str;		}		public static function toCamelCase(str:String):String {						var parts:Array = str.split(" ");						for(var i:int=0; i<parts.length; i++) {								var p:String = parts[i].toLowerCase();								parts[i] = p.charAt(0).toUpperCase() + p.substring(1,p.length);			}						return parts.join(" ");		}				/************************************************************************		*		*************************************************************************/		public static function getIndexKey(str:String):String {						if(str == null) return "";						var regEx1:RegExp = /\s/g;			var regEx2:RegExp = /[^\w]/g;						str = str.replace(regEx1,'_');			str = str.replace(regEx2,'');						return str.toLowerCase();		}	}}