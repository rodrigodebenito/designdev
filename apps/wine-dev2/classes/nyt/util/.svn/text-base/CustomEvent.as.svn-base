package nyt.util {	                 	import flash.events.Event;		import nyt.util.*;		public class CustomEvent extends Event {				public var info:Object;				public function CustomEvent(type:String,bubbles:Boolean=false,cancelable:Boolean=false) {						super(type,bubbles,cancelable);						info = new Object();		}				public override function clone():Event {						var ce:CustomEvent = new CustomEvent(type,bubbles,cancelable);						ce.info = info;						return ce;		}	}}